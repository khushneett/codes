   ],
   "source": [
    "# random shuffling of rows \n",
    "dfs = df_new.sample(frac = 1)\n",
    "dfs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 117,
   "id": "436e0b34",
   "metadata": {},
   "outputs": [],
   "source": [
    "# dropping the columns with labels\n",
    "x=dfs.drop(dfs.columns[-1],axis=1)\n",
    "\n",
    "# converting dataframe into array\n",
    "x=np.array(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 118,
   "id": "1207a077",
   "metadata": {},
   "outputs": [],
   "source": [
    "# slelcting all columns except the labels\n",
    "y=dfs[dfs.columns[-1]]\n",
    "\n",
    "# converting dataframe into array\n",
    "y=np.array(y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 119,
   "id": "e53e7288",
   "metadata": {},
   "outputs": [],
   "source": [
    "# fitting the data to 5-fold stratified cross validation\n",
    "skf = StratifiedKFold(n_splits=5,shuffle = True)  \n",
    "\n",
    "Xtr,Xv,ytr,yv=train_test_split(\n",
    "    x,\n",
    "    y,\n",
    "    test_size=0.4,\n",
    "    random_state=0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 120,
   "id": "2b15f692",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Mean ROC: 0.9840672222222222\n",
      "Std Dev ROC: 0.006846773007576974\n"
     ]
    }
   ],
   "source": [
    "# Kernelised SVM\n",
    "\n",
    "ksd_ROC=[]\n",
    "\n",
    "\n",
    "for i,(tr_idx, v_idx) in enumerate(skf.split(x, y)):\n",
    "    Xtr, Xv = x[tr_idx], x[v_idx]\n",
    "    ytr, yv = y[tr_idx], y[v_idx]\n",
    "    ksd = SVC(C = 1.0, kernel = 'poly', gamma = 1.0,  class_weight = 'balanced')\n",
    "    ksd.fit(Xtr,ytr)\n",
    "    predict_ksd=ksd.predict(Xv)\n",
    "    ksd_df=ksd.decision_function(Xv)\n",
    "    \n",
    "    fpr_ksd,tpr_ksd,tt_ksd = roc_curve(yv, ksd_df)\n",
    "    roc_auc = auc(fpr_ksd, tpr_ksd)\n",
    "    \n",
    "    # ROC\n",
    "    roc_ksd=roc_auc_score(yv,ksd_df)\n",
    "    ksd_ROC.append(roc_ksd)\n",
    "    \n",
    "\n",
    "# Mean ROC\n",
    "mean_roc_ksd=np.mean(ksd_ROC)\n",
    "print('Mean ROC:',np.mean(ksd_ROC))\n",
    "\n",
    "# Std Dev ROC                         \n",
    "stdev_roc_ksd=np.std(ksd_ROC)\n",
