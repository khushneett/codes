   "outputs": [],
   "source": [
    "# splitting the data\n",
    "Xtr,Xv,ytr,yv=train_test_split(\n",
    "    xtrain,\n",
    "    ytrain,\n",
    "    test_size=0.4,\n",
    "    random_state=0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 108,
   "id": "5dbac9e4",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Training set score: 0.9476666666666667\n",
      "Test set score: 0.9616666666666667\n"
     ]
    }
   ],
   "source": [
    "# creating a pipeline\n",
    "pipe = Pipeline([\n",
    "    ('scaler', StandardScaler()),\n",
    "    ('svc',SVC())\n",
    "])\n",
    "\n",
    "# fiting the data using the created pipeline\n",
    "pipe.fit(xtrain, ytrain)\n",
    " \n",
    "print('Training set score: ' + str(pipe.score(xtrain,ytrain)))\n",
    "print('Test set score: ' + str(pipe.score(Xv,yv)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 109,
   "id": "b586f5bd",
